  int lenc56 = (c56.__cid == String() ? length@String(c56) : 0);
  int lenc78 = (c78.__cid == String() ? length@String(c78) : 0);
  int lenc12c34c56c78 = lenc12 + lenc34 +lenc56 + lenc78;

  assert lenc12c34c56c78 == len12345678;
}
